            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_bank_customer_residence" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">M2</field>
            <field name="field_name">customer_residence</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

    </data>
</openerp>
