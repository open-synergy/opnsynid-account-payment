<openerp>
    <data>

        <!-- Template -->
        <record id="template_auto_transfer" model="base.xlsx.template">
            <field name="name">BCA Auto Transfer</field>
            <field name="model_id" ref="account_payment.model_payment_order"/>
            <field name="file_name">report_bca_auto_transfer.xlsx</field>
            <field name="file" type="base64" file="account_payment_banking_batch_processing_bca_auto_transfer/data/report_bca_auto_transfer.xlsx"/>
        </record>

        <!-- Sheet -->
        <record id="template_auto_transfer_sheet" model="base.xlsx.template.sheet">
            <field name="name">auto_transfer</field>
            <field name="template_id" ref="template_auto_transfer"/>
        </record>

        <!-- Sheet Detail -->
        <record id="template_auto_transfer_sheet_detail" model="base.xlsx.template.sheet.detail">
            <field name="field_name">report_bca_auto_transfer_ids</field>
            <field name="template_sheet_id" ref="template_auto_transfer_sheet"/>
        </record>

        <!-- Sheet Detail Detail -->
        <record id="template_auto_transfer_sheet_detail_trx_ref_no" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">A2</field>
            <field name="field_name">trx_ref_no</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_transac_date" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">B2</field>
            <field name="field_name">transac_date</field>
            <field name="field_cond">value or ''</field>
            <field name="style">style=date[mm/dd/yyyy]</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_acc_no_from" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">C2</field>
            <field name="field_name">acc_no_from</field>
