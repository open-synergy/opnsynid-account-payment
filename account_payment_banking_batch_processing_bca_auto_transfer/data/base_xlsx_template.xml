        <record id="template_auto_transfer_sheet_detail_acc_no_to" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">D2</field>
            <field name="field_name">acc_no_to</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_trans_amount" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">E2</field>
            <field name="field_name">trans_amount</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_remark1" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">F2</field>
            <field name="field_name">remark1</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_remark2" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">G2</field>
            <field name="field_name">remark2</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_jenis" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">H2</field>
            <field name="field_name">jenis</field>
            <field name="field_cond">value or ''</field>
            <field name="template_sheet_detail_id" ref="template_auto_transfer_sheet_detail"/>
        </record>

        <record id="template_auto_transfer_sheet_detail_bi_code" model="base.xlsx.template.sheet.detail.detail">
            <field name="cell">I2</field>
