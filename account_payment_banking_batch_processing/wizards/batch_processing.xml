<openerp>
<data>

<record id="payment_batch_processing_view_form" model="ir.ui.view">
    <field name="name">payment.batch_processing form</field>
    <field name="model">payment.batch_processing</field>
    <field name="arch" type="xml">
        <form>
            <group name="group_1" colspan="4" col="2">
                <field name="order_id" invisible="1"/>
                <field name="output_format"/>
            </group>
            <footer>
                <button name="action_confirm"
                    type="object"
                    string="Confirm"
                    class="oe_highlight"/>
                or
                <button special="cancel"
                        string="Cancel"
                        class="oe_link"/>
            </footer>
        </form>
    </field>
</record>
</data>
</openerp>
